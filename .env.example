# DagsHub
DAGSHUB_USER=YOUR_USER
DAGSHUB_REPO=lending-club-risk-project
# Official dagshub client env name:
DAGSHUB_USER_TOKEN=your_token_here
# Backward-compatible alias used by existing scripts
DAGSHUB_TOKEN=your_token_here

# MLflow (set automatically by dagshub.init)
MLFLOW_TRACKING_URI=
MLFLOW_TRACKING_USERNAME=
MLFLOW_TRACKING_PASSWORD=

# GitHub
GITHUB_REPO_URL=https://github.com/YOUR_USER/lending-club-risk-project.git
GIT_USER_NAME=Your Name
GIT_USER_EMAIL=you@example.com

# Google Drive (for DVC remote)
GDRIVE_FOLDER_ID=your_folder_id
# Recommended if default OAuth app is blocked:
GDRIVE_CLIENT_ID=
GDRIVE_CLIENT_SECRET=
# Alternative auth mode:
GDRIVE_SERVICE_ACCOUNT_JSON=

# Optional LLM assistant for Streamlit Chat with Data
GROK_API_KEY=your_xai_api_key_here
